@model IEnumerable<SMS.Areas.Attendance.Models.AttendanceUser>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Attendance/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FingerPrint)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AdminMobileNo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FingerPrint)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Phone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AdminMobileNo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AttendanceUserId }) |
            @Html.ActionLink("Details", "Details", new { id=item.AttendanceUserId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AttendanceUserId })
        </td>
    </tr>
}

</table>
